{
  "name": "font-select",
  "version": "1.0.0",
  "description": "Custom element for picking local fonts",
  "main": "index.js",
  "scripts": {
    "start": "npx http-server",
    "lint": "npx eslint . --ext .js,.mjs --fix --ignore-pattern dist/",
    "fix": "npx prettier --write ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tomayac/font-select.git"
  },
  "keywords": [
    "local",
    "fonts"
  ],
  "author": "Thomas Steiner (tomac@google.com)",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/tomayac/font-select/issues"
  },
  "homepage": "https://github.com/tomayac/font-select#readme",
  "devDependencies": {
    "eslint": "^7.1.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^6.11.0",
    "http-server": "^0.12.3",
    "prettier": "^2.0.5",
    "terser": "^4.7.0"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 2020,
      "sourceType": "module"
    },
    "extends": [
      "google",
      "prettier"
    ]
  }
}
